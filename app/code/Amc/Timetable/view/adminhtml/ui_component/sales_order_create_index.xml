<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="fullcalendar-scheduler-1.0.2/lib/fullcalendar.min.css"/>
        <css src="fullcalendar-scheduler-1.0.2/scheduler.min.css"/>
        <!--<link src="fullcalendar-scheduler-1.0.2/lib/fullcalendar.min.js"/>-->
        <!--<link src="fullcalendar-scheduler-1.0.2/scheduler.js"/>-->
    </head>
    <body>
        <referenceContainer name="js">
            <block class="Magento\Backend\Block\Template" name="order.create.timetable.js" template="Amc_Timetable::sales/order/create/timetable-js.phtml"/>
        </referenceContainer>
        <referenceBlock name="data">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Amc_Timetable::sales/order/create/data.phtml</argument>
            </action>
            <block class="Amc\Timetable\Block\Adminhtml\Order" name="timetable" template="Amc_Timetable::sales/order/create/timetable.phtml" />
        </referenceBlock>
    </body>
</page>
