<mapping strict="1">
    <wrapper>shipping</wrapper>
    <fields>
        <firstname>
            <selector>[name='shippingAddress[firstname]']</selector>
        </firstname>
        <lastname>
            <selector>[name='shippingAddress[lastname]']</selector>
        </lastname>
        <company>
            <selector>[name='shippingAddress[company]']</selector>
        </company>
        <street>
            <selector>[name='shippingAddress[street][0]']</selector>
        </street>
        <city>
            <selector>[name='shippingAddress[city]']</selector>
        </city>
        <region_id>
            <input>select</input>
            <selector>[name='shippingAddress[region_id]']</selector>
        </region_id>
        <postcode>
            <selector>[name='shippingAddress[postcode]']</selector>
        </postcode>
        <country_id>
            <input>select</input>
            <selector>[name='shippingAddress[country_id]']</selector>
        </country_id>
        <telephone>
            <selector>[name='shippingAddress[telephone]']</selector>
        </telephone>
    </fields>
</mapping>
